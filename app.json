{
  "expo": {
    "name": "ishee",
    "version": "1.4.1",
    "description": "Modern Classified Ads Portal",
    "slug": "ishee",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#7678ED",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f3f3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ishee.emadjumaah",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "uber",
          "waze"
        ],
        "NSCameraUsageDescription": "Enable ishee to access camera roll to take and upload photos for your profile or products list",
        "NSPhotoLibraryUsageDescription": "Enable ishee to access your photos and upload selected ones to show them in your profile or products list",
        "NSLocationWhenInUseUsageDescription": "Turn on location services to share your product's location and enable search products around you"
      }
    },
    "android": {
      "package": "com.ishee.emadjumaah",
      "versionCode": 4
    },
    "notification": {
      "icon": "./assets/nicon.png",
      "color": "#7678ED",
      "androidMode": "collapse",
      "androidCollapsedTitle": "ishee"
    },
    "packagerOpts": {
      "sourceExts": [
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-typescript-transformer/index.js"
    }
  }
}